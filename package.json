{
    "name": "lesson-on-coding-angular-webpack",
    "version": "1.0.0",
    "description": "Angular webpack starter",
    "scripts": {
        "start": "webpack-dev-server --inline --hot --progress --port  8080 --open",
        "test": "karma start",
        "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
        "build:aot": "ngc -p tsconfig-aot.json",
        "build-aot-prod": "npm run build:aot && npm run webpack-prod",
        "webpack-dev": "cross-env NODE_ENV=development webpack",
        "webpack-prod": "cross-env NODE_ENV=production webpack"
    },
    "license": "MIT",
    "dependencies": {
        "@angular-redux/store": "^6.5.7",
        "@angular/common": "~4.2.0",
        "@angular/compiler": "~4.2.0",
        "@angular/compiler-cli": "^4.4.4",
        "@angular/core": "~4.2.0",
        "@angular/forms": "~4.2.0",
        "@angular/http": "~4.2.0",
        "@angular/platform-browser": "~4.2.0",
        "@angular/platform-browser-dynamic": "~4.2.0",
        "@angular/platform-server": "^4.4.4",
        "@angular/router": "~4.2.0",
        "core-js": "^2.4.1",
        "redux": "^3.7.2",
        "redux-observable": "^0.16.0",
        "rxjs": "5.0.1",
        "tassign": "^1.0.0",
        "zone.js": "^0.8.4"
    },
    "devDependencies": {
        "@types/jasmine": "2.5.36",
        "@types/node": "^6.0.45",
        "angular2-template-loader": "^0.6.0",
        "awesome-typescript-loader": "^3.0.4",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.0",
        "babel-preset-es2015": "^6.24.1",
        "cross-env": "^5.0.5",
        "css-loader": "^0.26.1",
        "extract-text-webpack-plugin": "2.0.0-beta.5",
        "file-loader": "^0.9.0",
        "html-loader": "^0.4.3",
        "html-webpack-plugin": "^2.16.1",
        "jasmine-core": "^2.4.1",
        "karma": "^1.2.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-jasmine": "^1.0.2",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^2.0.1",
        "null-loader": "^0.1.1",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.5.2",
        "style-loader": "^0.13.1",
        "typescript": "2.3.4",
